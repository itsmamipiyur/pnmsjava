        <managed-bean-name>pages$ItemPage</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.ItemPage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$NewItemPage</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.NewItemPage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ItemPage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newItem</from-outcome>
            <to-view-id>/pages/NewItemPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$NewItemType</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.NewItemType</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
