        <navigation-case>
            <from-outcome>dishTypePage</from-outcome>
            <to-view-id>/pages/DishTypePage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$DishPage</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.DishPage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$NewDish</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.NewDish</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
