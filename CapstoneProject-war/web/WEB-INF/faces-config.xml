            <from-outcome>eventTypePage</from-outcome>
            <to-view-id>/pages/EventTypePage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/EventTypePage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newEventType</from-outcome>
            <to-view-id>/pages/NewEventTypePage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
