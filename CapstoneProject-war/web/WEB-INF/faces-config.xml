        <navigation-case>
            <from-outcome>decorPage</from-outcome>
            <to-view-id>/pages/DecorPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$DishTypePage</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.DishTypePage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$NewDishType</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.NewDishType</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/DishTypePage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newDishType</from-outcome>
            <to-view-id>/pages/NewDishType.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
