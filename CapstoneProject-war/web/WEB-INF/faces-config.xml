        <from-view-id>/pages/NewItemPage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newItemType</from-outcome>
            <to-view-id>/pages/NewItemType.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>itemPage</from-outcome>
            <to-view-id>/pages/ItemPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/NewItemType.jsp</from-view-id>
        <navigation-case>
            <from-outcome>newItem</from-outcome>
            <to-view-id>/pages/NewItemPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
