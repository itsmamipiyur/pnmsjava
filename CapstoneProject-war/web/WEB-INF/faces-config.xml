<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Page1</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.Page1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$AdminMenu</managed-bean-name>
        <managed-bean-class>capstoneprojectwar.pages.AdminMenu</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/AdminMenu.jsp</from-view-id>
        <navigation-case>
            <from-outcome>itemPage</from-outcome>
            <to-view-id>/pages/ItemPage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>eventTypePage</from-outcome>
            <to-view-id>/pages/EventTypePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>decorPage</from-outcome>
            <to-view-id>/pages/DecorPage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dishTypePage</from-outcome>
            <to-view-id>/pages/DishTypePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dishPage</from-outcome>
            <to-view-id>/pages/DishPage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cateringPackagePage</from-outcome>
            <to-view-id>/pages/CateringPackagePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>rentalPackagePage</from-outcome>
            <to-view-id>/pages/RentalPackagePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deliveryPage</from-outcome>
            <to-view-id>/pages/DeliveryPage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
